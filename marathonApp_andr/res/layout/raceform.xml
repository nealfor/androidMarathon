<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <TableLayout android:id="@+id/raceHeader"
	    android:layout_width="250dip"
	    android:layout_height="wrap_content"
	    android:layout_alignParentBottom="false"	 
	    android:layout_centerHorizontal="true"   
	    android:stretchColumns="0"
	    >
	<TableRow >		    
    <TextView android:id="@+id/eventName"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1"
		android:gravity="center_vertical"
		android:textStyle="bold"
		android:singleLine="false"
		android:text="Race Name Here" 
		android:ellipsize="end"
	/>
    </TableRow>	
    <TableRow >	
	<TextView android:id="@+id/eventType"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1"
		android:gravity="center_vertical"
		android:singleLine="false"
		android:text="Training Difficulty Here"
		android:ellipsize="end"
	/>
	</TableRow>
	<TableRow >	
	<TextView android:id="@+id/pace"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1"
		android:gravity="center_vertical"
		android:singleLine="false"
		android:text="-:30 pace"
		android:ellipsize="end"
	/>
	</TableRow>
	</TableLayout>	
	<View
        android:id="@+id/horizontalLine"
        android:layout_width="fill_parent"
        android:layout_height="2dip"
        android:background="#FF909090"
        android:layout_below="@+id/raceHeader" />
<ScrollView 
